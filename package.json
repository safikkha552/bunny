{
  "name": "server",
  "version": "1.0.0",
  "description": "server",
  "main": "index.js",
  "scripts": {
    "start": "cls && concurrently \"npm run server\" \"npm run client\"",
    "server": "set NODE_ENV=development && nodemon server/index.js",
    "server:dev": "set NODE_ENV=development && nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "cls && concurrently \"npm run server:dev\" \"npm run client\"",
    "build": "npm install && cd client && npm install && npm run build"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.4.0",
    "bitcoinjs-lib": "^6.1.3",
    "bitcore-explorers": "^1.0.1",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "execp": "^0.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.9.4",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "paypal-rest-sdk": "^1.8.1",
    "request": "^2.88.2",
    "socket.io": "^4.5.4",
    "stripe": "^12.18.0"
  },
  "nodemonConfig": {
    "ignore": [
      "server/json/*"
    ]
  }
}
